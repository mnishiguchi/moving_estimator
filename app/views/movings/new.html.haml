- provide(:title, "New moving")

%h1.page-header New moving

%ol.breadcrumb
  %li= link_to "My movings", root_path
  %li.active New moving

= form_for(@moving) do |f|

  = render 'shared/error_messages', object: f.object

  = render 'fields', f: f

  .form-group{ style: "width: 100%" }
    = f.button "Create moving", class: "submit btn btn-success btn-block",
      data: { disable_with: "<i class='fa fa-spinner fa-spin'></i> Processing..." }
